# ================================
# .NET + React project .gitignore
# ================================

# Ignore Visual Studio cache and settings
.vs/
.vscode/
*.user
*.suo
*.userprefs

# Ignore build outputs
bin/
obj/
out/
TestResults/
Debug/
Release/
*.exe
*.dll
*.pdb
*.cache
*.log

# Ignore NuGet and packages
packages/
*.nupkg
*.snupkg
project.lock.json
project.fragment.lock.json

# Ignore system or IDE generated files
*.db
*.db-shm
*.db-wal
*.sdf
*.ldf
*.mdf

# Ignore publish output
publish/
wwwroot/
dist/
node_modules/

# Ignore environment files
.env
.env.local
.env.development
.env.production
.env.test

# ================================
# Files to KEEP (tracked)
# ================================
!*.cs
!*.json
!*.csproj
!*.sln
!*.config
!*.xml
